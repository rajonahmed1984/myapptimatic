{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.1",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.9",
        "phpunit/phpunit": "^11.5",
        "spatie/laravel-ignition": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --ansi"
        ],
        "lint": [
            "@php vendor/bin/pint --test --dirty"
        ],
        "test": [
            "@php artisan test"
        ],
        "phase5:verify": [
            "@php artisan test --filter=UiParityTest",
            "@php artisan test --filter=ActionContractParityTest",
            "@php artisan test --filter=NoBreakSmokeTest",
            "npm run build",
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan test",
            "@php artisan optimize:clear --except=cache"
        ],
        "phase6:verify": [
            "@php artisan test --filter=PaymentCallbackContractTest",
            "@php artisan test --filter=PaymentProofActionContractParityTest",
            "@php artisan test --filter=PaymentGatewayActionContractParityTest",
            "@php artisan test --filter=ComplexModuleTransportContractTest",
            "@php artisan test --filter=NoBreakSmokeTest",
            "npm run build",
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan test",
            "@php artisan optimize:clear --except=cache"
        ],
        "phase7:verify": [
            "@php artisan test --filter=AccountingUiParityTest",
            "@php artisan test --filter=AutomationStatusUiParityTest",
            "@php artisan test --filter=AdminChatUiParityTest",
            "@php artisan test --filter=ApptimaticEmailUiParityTest",
            "@php artisan test --filter=ApptimaticEmailShowUiParityTest",
            "@php artisan test --filter=CommissionPayoutUiParityTest",
            "@php artisan test --filter=ExpenseCategoryUiParityTest",
            "@php artisan test --filter=ExpensesUiParityTest",
            "@php artisan test --filter=FinancePaymentMethodsUiParityTest",
            "@php artisan test --filter=FinanceReportsUiParityTest",
            "@php artisan test --filter=FinanceTaxUiParityTest",
            "@php artisan test --filter=IncomeCategoryUiParityTest",
            "@php artisan test --filter=IncomeUiParityTest",
            "@php artisan test --filter=LicenseUiParityTest",
            "@php artisan test --filter=OrderUiParityTest",
            "@php artisan test --filter=PaymentGatewayUiParityTest",
            "@php artisan test --filter=PaymentProofUiParityTest",
            "@php artisan test --filter=PlanUiParityTest",
            "@php artisan test --filter=ProductUiParityTest",
            "@php artisan test --filter=RecurringExpenseUiParityTest",
            "@php artisan test --filter=SubscriptionUiParityTest",
            "@php artisan test --filter=SupportTicketUiParityTest",
            "@php artisan test --filter=SystemLogUiParityTest",
            "@php artisan test --filter=UserActivitySummaryUiParityTest",
            "@php artisan test --filter=ClientInvoicesUiParityTest",
            "@php artisan test --filter=ClientSupportTicketUiParityTest",
            "@php artisan test --filter=ExpenseCategoryActionContractParityTest",
            "@php artisan test --filter=FinanceTaxActionContractParityTest",
            "@php artisan test --filter=IncomeCategoryActionContractParityTest",
            "@php artisan test --filter=PaymentGatewayActionContractParityTest",
            "@php artisan test --filter=PaymentProofActionContractParityTest",
            "@php artisan test --filter=RecurringExpenseActionContractParityTest",
            "@php artisan test --filter=ComplexModuleTransportContractTest",
            "@php artisan test --filter=NoBreakSmokeTest",
            "npm run build",
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan test",
            "@php artisan optimize:clear --except=cache"
        ],
        "ci": [
            "@lint",
            "@test"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "12.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
