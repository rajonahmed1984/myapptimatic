APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=false
APP_TIMEZONE=UTC
APP_URL=http://localhost
LOGIN_TRACE=false
TRUSTED_PROXIES=*
CRON_TOKEN=

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=myapptimatic
DB_USERNAME=root
DB_PASSWORD=root
DB_CHARSET=utf8mb4
DB_COLLATION=utf8mb4_unicode_ci

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_COOKIE=myapptimatic_session
SESSION_PATH=/
# For single-host production set exact host, e.g. my.apptimatic.com (no leading dot).
# Never use the parent domain (e.g. .apptimatic.com) if another app like WHMCS shares it.
SESSION_DOMAIN=null
SESSION_SECURE_COOKIE=false
SESSION_SAME_SITE=lax

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=sendmail
MAIL_SENDMAIL_PATH="/usr/sbin/sendmail -bs -i"
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

RECAPTCHA_SITE_KEY=
RECAPTCHA_SECRET_KEY=
RECAPTCHA_PROJECT_ID=
RECAPTCHA_API_KEY=
RECAPTCHA_SCORE_THRESHOLD=0.5

VITE_APP_NAME="${APP_NAME}"

GOOGLE_AI_ENABLED=true
GOOGLE_AI_API_KEY=
GOOGLE_AI_MODEL=gemini-1.5-flash
GOOGLE_AI_BASE_URL=https://generativelanguage.googleapis.com/v1beta
GOOGLE_AI_TIMEOUT=15
GOOGLE_AI_MAX_OUTPUT_TOKENS=600
GOOGLE_AI_TEMPERATURE=0.4
WHMCS_URL=
WHMCS_API_URL=
WHMCS_ADMIN_USERNAME=
WHMCS_API_IDENTIFIER=
WHMCS_API_SECRET=
WHMCS_TIMEOUT=10
FEATURE_REACT_SANDBOX=false
FEATURE_ADMIN_EXPENSES_RECURRING_INDEX=false
FEATURE_ADMIN_EXPENSES_RECURRING_SHOW=false
FEATURE_ADMIN_EXPENSES_RECURRING_CREATE=false
FEATURE_ADMIN_EXPENSES_RECURRING_EDIT=false
